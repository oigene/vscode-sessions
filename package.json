{
  "name": "sessions",
  "displayName": "Sessions",
  "description": "Workspace / session / project manager focused on usability and simplicity. Save and switch workspaces with a keystroke.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.39.0"
  },
  "publisher": "Oigene",
  "author": {
    "name": "Eugen K"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oigene/vscode-sessions.git"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "workspace management",
    "project management",
    "session management",
    "productivity"
  ],
  "activationEvents": [
    "*"
  ],
  "icon": "images/icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sessions.open",
        "title": "Open new session."
      },
      {
        "command": "sessions.save",
        "title": "Save a new session"
      },
      {
        "command": "sessions.delete",
        "title": "Delete sessions"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+alt+w",
        "command": "sessions.save"
      },
      {
        "key": "ctrl+alt+e",
        "command": "sessions.open"
      },
      {
        "key": "ctrl+alt+d",
        "command": "sessions.delete"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "glob": "^7.1.4",
    "mocha": "^10.2.0",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.2.0"
  }
}
